{
  "rewrites": [
    {
      "source": "/",
      "destination": "/siwft/login.html"
    },
    {
      "source": "/login",
      "destination": "/siwft/login.html"
    },
    {
      "source": "/cadastro",
      "destination": "/siwft/cadastro.html"
    },
    {
      "source": "/dashboard",
      "destination": "/siwft/index.html"
    }
  ],
  "routes": [
